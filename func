6:inline t_vec	vecAdd(t_vec a, t_vec b)
8:	return (t_vec){a.x + b.x, a.y + b.y, a.z + b.z};
14:inline t_vec	vecSub(t_vec a, t_vec b)
16:	return (t_vec){a.x - b.x, a.y - b.y, a.z - b.z};
